<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.polsri.paio">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_polsri"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_polsri_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Paio">
        <activity android:name=".menu.kondisiAir"></activity>
        <activity android:name=".menu.settings" />
        <activity android:name=".menu.prediction" />
        <activity android:name=".menu.CameraView" />
        <activity android:name=".menu.beriMakan" />
        <activity android:name=".MainActivity" />


        <activity android:name=".splash.splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>